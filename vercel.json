{
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/githubApi/:match*", "destination": "https://api.github.com/:match*" },
    { "source": "/githubDownload/:match*", "destination": "https://raw.githubusercontent.com/:match*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
